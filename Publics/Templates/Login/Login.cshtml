<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Project Management">
    <meta name="author" content="GAMASIS">
    <link rel="icon" href="@Url.Content("~/Content/Logos/icon.ico")">
    <title>
        Project Manager | @Page.Title
    </title>
    <!-- Moment -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="@Url.Content("~/Components/Bootstrap/css/bootstrap.min.css")">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="@Url.Content("~/Components/font-awesome/css/font-awesome.min.css")">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/Components/Admin/css/AdminLTE.css?v=" + DateTime.Now.ToString("yyyyMMddHHmmss"))">
    <!-- iCheck -->
    <link rel="stylesheet" href="@Url.Content("~/Components/iCheck/square/blue.css")">

    <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" rel="stylesheet" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- jQuery 2.2.3 -->
    <script src="@Url.Content("~/Components/jQuery/jquery-2.2.3.min.js")"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script>

    <!-- Bootstrap 3.3.6 -->
    <script src="@Url.Content("~/Components/Bootstrap/js/bootstrap.min.js")"></script>
    <!-- iCheck -->
    <script src="@Url.Content("~/Components/iCheck/icheck.min.js")"></script>
    <script>
        $(function () {
            $('input').iCheck({
                checkboxClass: 'icheckbox_square-blue',
                radioClass: 'iradio_square-blue',
                increaseArea: '20%' // optional
            });
        });
        var Root = "@Url.Content("~/")";
    </script>
    <script src="@Url.Content("~/Components/Dialog/Dialog.js")"></script>
</head>
<body class="hold-transition login-page">
    <div class="login-box">
        <div class="login-logo">
            <a href="javascript:void(0)">
                <img src="@Url.Content("~/Content/Logos/GamasisPMLogo.png")" />
            </a>
        </div>
        @RenderBody()
    </div>

    <!-- /.login-box -->
    @RenderSection("Scripts", required: false)


    <script src="@Url.Content("~/Components/Dialog/Dialog.js")"></script>

    <script>
        var Root = "@Url.Content("~/")";
    </script>
</body>
</html>
